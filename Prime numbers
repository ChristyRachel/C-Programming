#include <stdio.h>
int main()
{
    int n,i,j,p;
    printf("enter the limit : ");
    scanf("%d",&n);
    printf("prime numbers are\n");
    for(i=1;i<=n;i++)
    {
        p=0;
        for(j=1;j<=n;j++)
        {
            if (i%j==0)
            p++;
        }
        {
            if (p==2)
            printf("%d\n",i);
        } 
        }
return 0;
}
