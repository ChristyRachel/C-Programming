#include <stdio.h>

int main()
{
 int i,j;
 for(i=0;i<=6;i++) // total 6 rows for all letters
   {
      for(j=0;j<=50;j++) // total 6 coloums for all letters
         {
             // Using if and else if, write conditions for printing star based on the requirement
           if ( (i!=0) & (i!=6) & (j==0) ) 
                printf("*");
           else if ((i==0) &
                    ((j==1)|(j==2)|(j==3)|(j==4)|(j==6)|(j==11)|(j==13)|(j==14)|(j==15)|(j==16)|(j==17)|(j==20)|(j==21)|(j==22
                    )|(j==23)|(j==24)|(j==25)|(j==28)|(j==29)|(j==30)|(j==31)|(j==33)|(j==34)|(j==35)|(j==36)|(j==37)|(j==38)
                    |(j==40)|(j==47) ))
               printf("*");
           else if ( (j==6) | (j==11) | (j==13) | (j==22) | (j==23) | (j==35) | (j==36) )
               printf("*");
           else if( (i==3) &
                    ((j==7)|(j==8)|(j==9)|(j==10)|(j==14)|(j==15)|(j==16)|(j==17)|(j==28)|(j==29)|(j==30)|(j==43) ))
               printf("*");
           else if( (i==6) & ((j==1)|(j==2)|(j==3)|(j==4)|(j==18)|
                    (j==20)|(j==21)|(j==22)|(j==23)|(j==24)|(j==25)|(j==27)|(j==28)|(j==29)|(j==30)|(j==43)))
               printf("*");
           else if( (j==18) & ((i==1)|(i==2)|(i==4)|(i==5) ))
               printf("*");
           else if( (j==27) & ((i==1)|(i==2)) )
               printf("*");
           else if( (j==31) & ((i==4)|(i==5)) )
               printf("*");
           else if( (i==1) & ((j==41)|(j==46)) )
               printf("*");
           else if( (i==2) & ((j==42)|(j==43)|(j==44)|(j==45)) )
               printf("*");
           else if(((j==43)|(j==44)) & ((i==4)|(i==5)))
               printf("*");
           else if((j==44) & ((i==3)|(i==6)))
               printf("*");
               
            // Using else, print space for other portions
               
           else 
               printf(" ");
      }
    printf("\n");
   }
 return 0;
}
