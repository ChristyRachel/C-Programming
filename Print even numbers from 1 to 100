#include<stdio.h>

int main()
{
    int i;//to initialize i
    for(i=1;i<=100;i++) //increament it till 100
    {
        if(i%2==0)//if the remainder is zero, when a number divided by 2, then it is an even number
          {
              printf("%d\n",i);//then print the number
          }
    }
return 0;//exit code
}
